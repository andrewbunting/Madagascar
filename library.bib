eprint = {9801280},
file = {:Users/Shared/bunting/Library/Application Support/Mendeley Desktop/Downloaded/Terquem et al. - 1998 - On the Tidal Interaction of a Solar‐Type Star with an Orbiting Companion Excitation of g ‐Mode Oscillatio(2).pdf:pdf},
issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {HYDRODYNAMICS,Hydrodynamics,STARS: BINARIES: CLOSE,STARS: INTERIORS,STARS: LATE-TYPE,STARS: OSCILLATIONS,STARS: PLANETARY SYSTEMS,Stars: Binaries: Close,Stars: Interiors,Stars: Late-Type,Stars: Oscillations,Stars: Planetary Systems,WAVES,Waves},
